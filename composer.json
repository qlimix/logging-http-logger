{
    "name": "qlimix/logging-http-logger",
    "type": "library",
    "description": "Logging PSR-7 http requests",
    "license": "MIT",
    "keywords": [
        "PSR-7",
        "logging",
        "request",
        "response"
    ],
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">7.4",
        "qlimix/logging" : "^3.0",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\Log\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "qlimix/code-standard": "^3.0"
    }
}
